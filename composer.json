{
    "name": "packback/prices",
    "type": "library",
    "description": "Making it simple to integrate your application with textbook price apis.",
    "keywords": [
        "textbooks",
        "prices",
        "api client",
        "object"
    ],
    "homepage": "https://github.com/Packback/prices",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Karl Hughes",
            "email": "khughes.me@gmail.com",
            "homepage": "http://www.karllhughes.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.6",
        "phpunit/php-code-coverage": "~2.0",
        "mockery/mockery": ">=0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Packback\\Prices\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Packback\\Prices\\Test\\": "test/src/"
        }
    }
}
